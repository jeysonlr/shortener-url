    "class-validator": "^0.12.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "exceljs": "^4.2.0",
    "highlight.js": "^10.5.0",
    "node-notifier": "^9.0.0",
    "node-xlsx": "^0.15.0",
    "pg": "^8.5.1",
